local tweenService = game:GetService("TweenService")
local player = game.Players.LocalPlayer


local function safeTeleport(targetCFrame)
    local character = player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return end
    
    local hrp = character.HumanoidRootPart
    local distance = (targetCFrame.Position - hrp.Position).Magnitude
    local speed = 250
    local duration = distance / speed

    local bv = Instance.new("BodyVelocity")
    bv.Velocity = Vector3.new(0, 0, 0)
    bv.MaxForce = Vector3.new(9e9, 9e9, 9e9)
    bv.Parent = hrp


    local noclipLoop = game:GetService("RunService").Stepped:Connect(function()
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end)

    local tween = tweenService:Create(hrp, TweenInfo.new(duration, Enum.EasingStyle.Linear), {CFrame = targetCFrame})
    
    tween:Play()
    
    tween.Completed:Connect(function()
        bv:Destroy()
        noclipLoop:Disconnect()

        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end        end
    end)
end


for _, location in ipairs(locations) do
    TeleportSection:Button({
        Title = location.name,
        Callback = function()
            safeTeleport(location.cframe)
        end,
    })
end


local WaterPart = Instance.new("Part")
WaterPart.Size = Vector3.new(100, 0.5, 100) -- بسیار نازک
WaterPart.Transparency = 1
WaterPart.Anchored = true
WaterPart.CanCollide = false
WaterPart.Parent = workspace

local function UpdateWaterWalk(value)
    _G.WaterWalk = value
    if value then
        task.spawn(function()
            while _G.WaterWalk do
                local char = game.Players.LocalPlayer.Character
                local hrp = char and char:FindFirstChild("HumanoidRootPart")
                local hum = char and char:FindFirstChild("Humanoid")
                
                if hrp and hum then
                    local waterLevel = -2 -- سطح آب
                    local currentY = hrp.Position.Y
                    
                    -- اگر زیر سطح آب رفتید، بلافاصله به بالا برگردانید
                    if currentY < waterLevel then
                        WaterPart.CanCollide = false
                        -- نیروی قوی برای بیرون آمدن سریع
                        hrp.Velocity = Vector3.new(hrp.Velocity.X, 100, hrp.Velocity.Z)
                        hrp.CFrame = CFrame.new(hrp.Position.X, waterLevel + 0.5, hrp.Position.Z)
                    else
                        -- قرارگیری دقیق روی سطح آب
                        WaterPart.CanCollide = true
                        -- پارت دقیقاً روی 0 (سطح آب)
                        WaterPart.CFrame = CFrame.new(hrp.Position.X, -5, hrp.Position.Z)
                    end
                    
                    -- جلوگیری از شنا
                    if hum:GetState() == Enum.HumanoidStateType.Swimming then
                        hum:ChangeState(Enum.HumanoidStateType.Running)
                    end
                end
                task.wait()
            end
            WaterPart.CanCollide = false
        end)
    else
        WaterPart.CanCollide = false
    end
end

local WaterWalk = Practical:Toggle({
    Title = "راه‌رفتن روی آب",
    Value = true,
    Callback = UpdateWaterWalk
})

if WaterWalk.Value then
    UpdateWaterWalk(true)
end

local Toggle = Practical:Toggle({
    Title = "Fast Attack / Kill Aura",
    Callback = function(state)
        _G.FastAttack = state
        
        task.spawn(function()
            local Net = game:GetService("ReplicatedStorage"):WaitForChild("Modules"):WaitForChild("Net")
            local RegisterAttack = Net:WaitForChild("RE/RegisterAttack")
            local RegisterHit = Net:WaitForChild("RE/RegisterHit")
            
            while _G.FastAttack do
                pcall(function()
                    local character = game.Players.LocalPlayer.Character
                    local root = character and character:FindFirstChild("HumanoidRootPart")
                    
                    if root and character:FindFirstChildOfClass("Tool") then
                        -- اجرای انیمیشن حمله
                        RegisterAttack:FireServer(0.4)
                        
                        -- پیدا کردن دشمنان و ثبت ضربه (Kill Aura)
                        for _, enemy in pairs(workspace.Enemies:GetChildren()) do
                            local enemyRoot = enemy:FindFirstChild("HumanoidRootPart")
                            local humanoid = enemy:FindFirstChild("Humanoid")
                            
                            if enemyRoot and humanoid and humanoid.Health > 0 then
                                local dist = (enemyRoot.Position - root.Position).Magnitude
                                if dist < 60 then
                                    -- استفاده از اطلاعاتی که فرستادید برای ثبت ضربه
                                    local args = {
                                        [1] = enemyRoot,
                                        [2] = {enemy},
                                        [4] = "694ba46d" -- کد هش اختصاصی که فرستادید
                                    }
                                    RegisterHit:FireServer(unpack(args))
                                end
                            end
                        end
                    end
                end)
                task.wait(0.1) -- سرعت حمله (قابل تغییر)
            end
        end)
    end})



    -- تابع کمکی برای ایجاد متن ESP
local function CreateESP(part, name, color)
    if not part:FindFirstChild("ESP_UI") then
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP_UI"
        billboard.AlwaysOnTop = true
        billboard.Size = UDim2.new(0, 100, 0, 50)
        billboard.ExtentsOffset = Vector3.new(0, 3, 0)
        billboard.Parent = part

        local label = Instance.new("TextLabel")
        label.Parent = billboard
        label.BackgroundTransparency = 1
        label.Size = UDim2.new(1, 0, 1, 0)
        label.Text = name
        label.TextColor3 = color
        label.TextSize = 14
        label.TextStrokeTransparency = 0
    end
end

-- تابع حذف ESP
local function RemoveESP(tag)
    for _, v in pairs(workspace:GetDescendants()) do
        if v.Name == "ESP_UI" and v.Parent and (tag == "All" or v:FindFirstChild("TextLabel").Text:find(tag)) then
            v:Destroy()
        end
    end
end

-- ESP Players
local PlayerESP = Practical:Toggle({
    Title = "ESP Players",
    Callback = function(state)
        _G.ESP_Players = state
        if not state then RemoveESP("") end -- حذف همه در صورت خاموش شدن (نیاز به منطق دقیق‌تر برای تفکیک دارد)
        task.spawn(function()
            while _G.ESP_Players do
                for _, p in pairs(game.Players:GetPlayers()) do
                    if p ~= game.Players.LocalPlayer and p.Character and p.Character:FindFirstChild("Head") then
                        CreateESP(p.Character.Head, p.Name, Color3.fromRGB(255, 0, 0))
                    end
                end
                task.wait(2)
            end
        end)
    end
})

-- ESP Chests
local ChestESP = Practical:Toggle({
    Title = "ESP Chests",
    Callback = function(state)
        _G.ESP_Chests = state
        if not state then 
            for _, v in pairs(workspace:GetChildren()) do 
                if v.Name:find("Chest") and v:FindFirstChild("ESP_UI") then v.ESP_UI:Destroy() end 
            end
        end
        task.spawn(function()
            while _G.ESP_Chests do
                for _, v in pairs(workspace:GetChildren()) do                    if v.Name:find("Chest") and v:IsA("BasePart") then
                        CreateESP(v, "Chest", Color3.fromRGB(255, 255, 0))
                    end
                end
                task.wait(5)
            end
        end)
    end
})

-- ESP Islands
local IslandESP = Practical:Toggle({
    Title = "ESP Islands",
    Callback = function(state)
        _G.ESP_Islands = state
        if not state then RemoveESP("Island") end        task.spawn(function()
            while _G.ESP_Islands do
                -- در Blox Fruits جزایر معمولاً در Locations یا مدل‌های بزرگ در Workspace هستند
                local locs = workspace:FindFirstChild("_WorldOrigin") and workspace._WorldOrigin:FindFirstChild("Locations")
                if locs then
                    for _, v in pairs(locs:GetChildren()) do
                        CreateESP(v, v.Name, Color3.fromRGB(0, 255, 255))
                    end
                end
                task.wait(10)
            end
        end)
    end
})

-- ESP Fruits
local FruitESP = Practical:Toggle({
    Title = "ESP Fruits",
    Callback = function(state)
        _G.ESP_Fruits = state
        if not state then 
             for _, v in pairs(workspace:GetChildren()) do 
                if v.Name:find("Fruit") and v:FindFirstChild("ESP_UI") then v.ESP_UI:Destroy() end 
            end
        end
        task.spawn(function()            while _G.ESP_Fruits do
                for _, v in pairs(workspace:GetChildren()) do                    if v:IsA("Tool") and v.Name:find("Fruit") then -- میوه‌های در دست یا روی زمین
                        local handle = v:FindFirstChild("Handle")
                        if handle then CreateESP(handle, "Fruit", Color3.fromRGB(0, 255, 0)) end
                    elseif v:IsA("Model") and v.Name:find("Fruit") then -- میوه‌های اسپاون شده
                        local part = v:FindFirstChildOfClass("BasePart")
                        if part then CreateESP(part, "Fruit", Color3.fromRGB(0, 255, 0)) end
                    end
                end
                task.wait(3)
            end
        end)
    end
})
