    Title  = "Vitex Hub",
    Icon   = "aperture",
    Author = "by Vitex",
    Theme  = "Violet",
    Acrylic = true,
})

Window:Tag({
    Title = "Beta-kooon",
    Icon = "history",
    Color = Color3.fromHex("#5f0188"),
    Radius = 0,
})

