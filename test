        else
            -- Ù†Ù…Ø§ÛŒØ´ Ø§Ø±ÙˆØ± Ø¯Ø± ØµÙˆØ±Øª Ù¾ÛŒØ¯Ø§ Ù†Ø´Ø¯Ù†
            local ErrorNotify = WindUI:Notify({
                Title = "Ø®Ø·Ø§",
                Content = "Ù…Ú©Ø§Ù† Ù…ÙˆØ±Ø¯ Ù†Ø¸Ø± Ù¾ÛŒØ¯Ø§ Ù†Ø´Ø¯!",
                Duration = 3
